---
import Logo from 'src/components/Logo.astro'
import Layout from 'src/layouts/Layout.astro'

const props = Astro.props
---

<Layout title="404: Not Found" description={`Path not found: ${props}`} errorPage>
  <Logo class="text-5xl py-12" />
  <p class="text-2xl font-mono mb-4">
    <span class="text-3xl font-black text-secondary">404:</span> Page Not found
  </p>
</Layout>
